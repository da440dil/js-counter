{
  "name": "@da440dil/js-counter",
  "version": "0.6.0",
  "description": "Distributed rate limiting using Redis",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext .js,.ts src",
    "build": "rimraf lib && tsc -p tsconfig.build.json && copyfiles -f \"src/*.lua\" lib",
    "preversion": "npm run lint && npm run test",
    "prepublishOnly": "npm run build",
    "file": "ts-node",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [
    "distributed",
    "rate",
    "rate-limit",
    "rate-limiter",
    "rate-limiting",
    "redis"
  ],
  "author": "Anatoly Demidovich",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10.0"
  },
  "dependencies": {
    "@da440dil/js-redis-script": "^0.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/redis": "2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.23.0",
    "jest": "^26.6.3",
    "redis": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/da440dil/js-counter.git"
  }
}
