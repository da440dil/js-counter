    "@types/node": "^12.0.3",
    "@types/redis": "2.8.13",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/da440dil/js-counter.git"
  }
