{
  "name": "@da440dil/js-counter",
  "version": "0.4.0",
  "description": "Distributed rate limiting using Redis",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jest.config.json --runInBand",
    "test:verbose": "jest --config jest.config.json --runInBand --verbose",
    "test:coverage": "jest --config jest.config.json --runInBand --coverage",
    "lint": "eslint --ext .js,.ts src",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf",
    "preversion": "npm run lint && npm run test",
    "prepublishOnly": "npm run clean lib && npm run build",
    "file": "ts-node",
    "coveralls": "jest --config jest.config.json --runInBand --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [
    "distributed",
    "rate",
    "rate-limit",
    "rate-limiter",
    "rate-limiting",
    "redis"
  ],
  "author": "Anatoly Demidovich",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10.0"
  },
  "dependencies": {
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.10",
    "@types/redis": "2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/da440dil/js-counter.git"
  }
}
