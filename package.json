{
  "name": "@da440dil/js-counter",
  "version": "0.2.0",
  "description": "Distributed rate limiting using Redis",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jest.config.json --runInBand",
    "test:v": "jest --config jest.config.json --runInBand --verbose",
    "test:f": "jest --config jest.config.json --runInBand --",
    "test:c": "jest --config jest.config.json --runInBand --coverage",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf --",
    "preversion": "npm run lint && npm run test",
    "prepublishOnly": "npm run clean lib && npm run build",
    "file": "ts-node --",
    "coveralls": "jest --config jest.config.json --runInBand --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [
    "distributed",
    "rate",
    "rate-limit",
    "rate-limiter",
    "rate-limiting",
    "redis"
  ],
  "author": "Anatoly Demidovich",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/redis": "2.8.17",
    "coveralls": "^3.0.11",
    "jest": "^25.3.0",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/da440dil/js-counter.git"
  }
}
