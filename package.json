{
  "name": "@da440dil/js-counter",
  "version": "0.0.9",
  "description": "Distributed rate limiting using Redis",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jest.config.json --runInBand",
    "test:v": "jest --config jest.config.json --runInBand --verbose",
    "test:f": "jest --config jest.config.json --runInBand --",
    "test:c": "jest --config jest.config.json --runInBand --coverage",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build": "tsc -p tsconfig.build.json",
