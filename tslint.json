{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "max-line-length": [
      true,
      150
    ],
    "member-access": [
      false
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "max-classes-per-file": false,
    "interface-name": false,
    "no-console": false
  },
  "rulesDirectory": []
}